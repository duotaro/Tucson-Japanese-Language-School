{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS=--max_old_space_size=8192 next build && next export && next-sitemap --config next-sitemap.js",
    "start": "next start",
    "sitemap": "next-sitemap --config next-sitemap.js",
    "lint": "next lint",
    "deploy": "next-sitemap --config next-sitemap.js && git add . && git commit -m 'release' && git push origin develop",
    "release": "next-sitemap --config next-sitemap.js && git add . && git commit -m 'release' && git push origin main",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@headlessui/react": "^2.1.5",
    "@heroicons/react": "^2.1.5",
    "@notionhq/client": "^2.2.3",
    "@popperjs/core": "^2.11.8",
    "@splidejs/react-splide": "^0.7.12",
    "@tailwindcss/line-clamp": "^0.4.4",
    "jquery": "^3.7.1",
    "next": "^13.5.6",
    "next-sitemap": "^4.0.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-twitter-embed": "^4.0.4",
    "resend": "^4.0.0",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@next/bundle-analyzer": "^15.3.5",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "dotenv": "^17.0.0",
    "eslint": "8",
    "eslint-config-next": "13",
    "postcss": "^8.4.45",
    "serve": "^14.2.0",
    "tailwindcss": "^3.4.10",
    "vercel": "^44.2.8",
    "wrangler": "^4.22.0"
  },
  "packageManager": "yarn@4.2.2"
}
